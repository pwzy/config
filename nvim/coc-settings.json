{

    // setting for coc-explore
    "explorer.width": 35,
    "explorer.icon.enableNerdfont": true,
    "explorer.previewAction.onHover": false,
    "explorer.keyMappings": {
        "<cr>": ["expandable?", "expand", "open"],
        "v": "open:vsplit"
    },

    // setting for coc-jedi
    // "jedi.executable.command": "~/anaconda3/envs/torch_1_12/bin/jedi-language-server",
    "jedi.diagnostics.enable": false,
    
    // setting for coc-pyright
    "pyright.disableDiagnostics": true

}
